<script>
    $(document).ready(function () {
        $('.filter-button').click(function () {
            var filter = $(this).attr('data-filter');
            $('.filter-button').removeClass('active');
            $(this).addClass('active');
            if (filter == '*') {
                $('.item').show();
            } else {
                $('.item').hide();
                $(filter).show();
            }
        });
    });
</script>