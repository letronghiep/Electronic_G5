    <section id="all" class="my-5">
        <div class="container my-5 py-5">

            <div class="section-header d-md-flex justify-content-between align-items-center">
                <h2 class="display-3 fw-normal">Sản phẩm</h2>
                @*<div>
                    <a href="#" class="btn btn-outline-dark btn-lg text-uppercase fs-6 rounded-1">
                        Mua ngay
                        <svg width="24" height="24" viewBox="0 0 24 24" class="mb-1">
                            <use xlink:href="#arrow-right"></use>
                        </svg>
                    </a>
                </div>*@
            </div>
            <div class="mb-4 mb-md-0">
                <p class="m-0">
                    <button class="filter-button me-4  active" data-filter="*">Tất cả sản phẩm</button>
                    <button class="filter-button me-4 " data-filter=".new">Sản phẩm mới nhất</button>
                    <button class="filter-button me-4 " data-filter=".bestSeller">Sản phẩm bán chạy</button>
                    <button class="filter-button me-4 " data-filter=".sort">Sắp xếp theo giá</button>
                </p>
            </div>
            <div class="isotope-container row">
                <!--all-->
                @foreach (var product in Model.AllProducts)
                {
                    var imageUrl = "~/wwwroot/Images/" + product.Product.image_url;
                    <div class=" item all col-md-4 col-lg-3 my-4">
                        <div class="card position-relative">
                            <a href="@Url.Action("ChiTietSP","Home", new {product_id = product.Product.product_id})">
                                @if (product.Product.image_url != null)
                                {
                                    <img src="@Url.Content(imageUrl)" class="img-fluid rounded-4" alt="ảnh">
                                }
                                else
                                {
                                    <img src="default-image.jpg" class="img-fluid rounded-4" alt="Default Image">
                                }
                            </a>
                            <div class="card-body p-0">
                                <a href=" @Url.Action("ChiTietSP","Home")">
                                    <h3 class="card-title pt-4 m-0">@product.Product.product_name</h3>
                                </a>
                                <div class="card-text">
                                    <h3 class="secondary-font text-primary">$18.00</h3>
                                    <div class="d-flex flex-wrap mt-3">
                                        <a href="#" class="btn-cart me-3 px-4 pt-3 pb-3">
                                            <button class="text-uppercase m-0">Thêm vào giỏ hàng</button>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
                <!--Latest-->
                @foreach (var product in Model.LatestProducts)
                {
                    var imageUrl = "~/wwwroot/Images/" + product.Product.image_url;
                    <div class="item new col-md-4 col-lg-3 my-4">
                        <div class="card position-relative">
                            <a href="@Url.Action("ChiTietSP","Home", new {product_id = product.Product.product_id})">
                                @if (product.Product.image_url != null)
                                {
                                    <img src="@Url.Content(imageUrl)" class="img-fluid rounded-4" alt="@product.Product.product_name">
                                }
                                else
                                {
                                    <img src="default-image.jpg" class="img-fluid rounded-4" alt="Default Image">
                                }
                            </a>
                            <div class="card-body p-0">
                                <a href="@Url.Action("ChiTietSP","Home", new {product_id = product.Product.product_id})">
                                    <h3 class="card-title pt-4 m-0">@product.Product.product_name</h3>
                                </a>
                                <div class="card-text">
                                    <h3 class="secondary-font text-primary">$18.00</h3>
                                    <div class="d-flex flex-wrap mt-3">
                                        <a href="#" class="btn-cart me-3 px-4 pt-3 pb-3">
                                            <button class="text-uppercase m-0">Thêm vào giỏ hàng</button>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }

                <!--bestseller-->
                <!--sort-->
            </div>

        </div>
